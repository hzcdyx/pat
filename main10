#include <iostream>
#include <string>
using namespace std;
int main(int argc, char** argv) {
	int a,b,c,max[5],s;
	cin>>a>>b>>c;
	s=a>b?a:b;
	max[2]=s>c?s:c;
	if(max[2]==c)
	{
		max[1]=a>b?a:b;
		if(max[1]==a){
			max[0]=b;
		}
		else{
			max[0]=a;
		}
	}
	else if(max[2]==b)
	{
		max[1]=a>c?a:c;
		if(max[1]==a){
			max[0]=c;
		}
		else{
			max[0]=a;
		}	
	}
	else{
		max[1]=b>c?b:c;
		if(max[1]==b){
			max[0]=c;
		}
		else{
			max[0]=b;
		}	
	}
	cout<<max[0]<<"->"<<max[1]<<"->"<<max[2];
	return 0;
}
