#include <iostream>
#include <bits/stdc++.h>
using namespace std; 
string s;
int len=0;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int main(int argc, char** argv) {
	int x=1,y=0,i,j,n,t,num=0;
	int a[5000];
	cin>>n;
	while(true)
	{
		len++;
	if(x/n||num)	
	{
		a[num++]=x/n;
		x=x%n;
		if(x==0)
		{
		break;
		}
		
	}
	x=x*10+1;
	}
	for(i=0;i<num;i++)
	{
		cout<<a[i];
	}
	cout<<" ";
	cout<<len;
	return 0;
}
