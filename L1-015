#include <bits/stdc++.h>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
  int a,i,j,n,m,f,nu=0;
  char b;
  cin>>n;
  cin>>b;
  m=1.0*n/2+0.5;
  for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
    {
      cout<<b;
    }
    cout<<endl;
  }
  return 0;
}
