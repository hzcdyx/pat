#include <bits/stdc++.h>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
int num[150];
int main(int argc, char** argv) {
	int a,i,j,m,f,nu=0;
	string n;
	char b;
	memset(num,0,sizeof(num));
	cin>>n;
	for(i=0;i<n.size();i++)
	{
		num[n[i]-'0']++;
	}
	for(i=0;i<10;i++)
	{
		if(num[i]!=0)
		{
			cout<<i<<':'<<num[i]<<endl;
		}
	}
	return 0;
}
