#include <bits/stdc++.h>
using namespace std; 
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int a,b,i,sum=0,t=0;
	cin>>a;
	cin>>b;
	for(i=a;i<=b;i++)
	{
		sum+=i;
		t++;
		if(t%5==0)
		{
		printf("%5d\n",i);	
	
		}
		else{
		printf("%5d",i);		
		}
		
	}
	if(t%5!=0)
	{
			printf("\n");
	}

	printf("Sum = %d",sum);
	return 0;
}
