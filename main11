#include <iostream>
#include <string>
using namespace std;
int main(int argc, char** argv) {
	string a;
	string b;
	getline(cin,a);
	getline(cin,b);
	
	int i,j;
	for(i=0;i<a.length();i++)
	{
		char d=a[i];
		for(j=0;j<b.length();j++)
		{
			if(b[j]==d)
			{
				break;
			}
		}
		if(j==b.length())
		{
			cout<<d;
		}
	}
	return 0;
}
